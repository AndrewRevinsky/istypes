{
  "name": "istypes",
  "version": "1.1.0",
  "description": "type-checking utility library for signaling type-match/mismatch and grouping items by type",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "mocha --require @babel/register --require @babel/core tests/**/*-test.js",
    "test:watch": "npm run test -- --watch src/**/*-test.js",
    "compile": "babel src --out-dir lib --ignore *-test.js",
    "prepublish": "npm run compile",
    "preversion": "npm run compile"
  },
  "repository": {
    "url": "git+https://github.com/andrevinsky/istypes.git",
    "type": "git"
  },
  "keywords": [
    "javascript",
    "type",
    "types",
    "type",
    "checking",
    "typecheck",
    "check",
    "checks",
    "checking",
    "is",
    "is-type"
  ],
  "author": "Andrew Revinsky <arevinsky@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrevinsky/istypes/issues"
  },
  "homepage": "https://github.com/andrevinsky/istypes#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "chai": "^4.1.2",
    "mocha": "^5.2.0"
  }
}
